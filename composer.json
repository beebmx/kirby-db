{
    "name": "beebmx/kirby-db",
    "description": "Enable Kirby 3 database support for Illuminate\\Database",
    "keywords": [
        "kirby", "kirby-3", "database", "model", "eloquent"
    ],
    "version": "1.0.4",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Gutierrez",
            "email": "fernando@beeb.mx"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "getkirby/composer-installer": "^1.2"
    },
    "require-dev": {
        "illuminate/database": "^8.0",
        "spatie/laravel-translatable": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "bundle/"
    },
    "autoload": {
        "files": [
            "config.php"
        ],
        "psr-4": {
            "Beebmx\\KirbyDB\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
